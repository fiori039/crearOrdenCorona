<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	
	<SelectDialog
		id="list_ValorCaractMaterial"
		title="Seleccione Valor Característica"
		class="sapUiPopupWithPadding"
		placeholder="Seleccione característica"
		
		items="{
			path : '/ValueCarctMaterialSet'
		}"
		
		
		confirm="f_confirm_valor_caracteristica"
		cancel="f_confirm_valor_caracteristica"
		>
		
		<StandardListItem
			title="{Value}"
			description="{Descr}"/>
	</SelectDialog>
</core:FragmentDefinition>